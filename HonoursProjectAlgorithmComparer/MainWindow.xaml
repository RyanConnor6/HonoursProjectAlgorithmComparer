        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HonoursProjectAlgorithmComparer"
        mc:Ignorable="d"
        Title="Algorithm Comparer" Height="1000" Width="1800">
    <Grid>
        <Canvas x:Name="canContainer" HorizontalAlignment="Left" Height="750" VerticalAlignment="Center" Width="750" Margin="950,0,0,0"/>
        <Button x:Name="startBtn" Content="Start" HorizontalAlignment="Left" Height="131" Margin="153,0,0,0" VerticalAlignment="Center" Width="309" FontSize="50" FontFamily="Arial Rounded MT Bold" Click="startBtn_Click"/>
    </Grid>
</Window>
