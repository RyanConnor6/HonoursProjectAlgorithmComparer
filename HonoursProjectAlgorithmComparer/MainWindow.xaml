<Window x:Class="HonoursProjectAlgorithmComparer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HonoursProjectAlgorithmComparer"
        mc:Ignorable="d"
        Title="Algorithm Comparer" Height="1000" Width="1800">
    <Grid>
        <Canvas x:Name="canContainer" HorizontalAlignment="Left" Height="750" VerticalAlignment="Center" Width="750" Margin="951,0,0,0" RenderTransformOrigin="0.5,0.5">
            <Canvas.RenderTransform>
                <TransformGroup>
                    <ScaleTransform ScaleX="-1"/>
                    <SkewTransform/>
                    <RotateTransform Angle="180"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Canvas.RenderTransform>
        </Canvas>
        <Button x:Name="startBtn" Content="Start" HorizontalAlignment="Left" Height="131" Margin="153,277,0,0" VerticalAlignment="Top" Width="309" FontSize="50" FontFamily="Arial Rounded MT Bold" Click="startBtn_Click"/>
        <Button x:Name="sizeBtn" Content="Test Nbrs" HorizontalAlignment="Left" Height="131" Margin="153,700,0,0" VerticalAlignment="Top" Width="309" FontSize="50" FontFamily="Arial Rounded MT Bold" Click="sizeBtn_Click"/>
        <TextBox x:Name="coordBox1" HorizontalAlignment="Left" Height="132" Margin="153,563,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="155" FontSize="50" FontFamily="Arial Rounded MT Bold"/>
        <TextBox x:Name="coordBox2" HorizontalAlignment="Left" Height="132" Margin="307,563,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="155" FontSize="50" FontFamily="Arial Rounded MT Bold"/>
        <Label Content="Create Grid" HorizontalAlignment="Left" Height="69" Margin="153,193,0,0" VerticalAlignment="Top" Width="309" FontSize="50" FontFamily="Arial Rounded MT Bold"/>
        <Label Content="Test Connections" HorizontalAlignment="Left" Height="69" Margin="93,479,0,0" VerticalAlignment="Top" Width="450" FontSize="50" FontFamily="Arial Rounded MT Bold"/>
    </Grid>
</Window>
